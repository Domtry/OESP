{
  "name": "@oesp/all",
  "private": false,
  "version": "7.0.0",
  "description": "Meta-package including all OESP TypeScript SDK modules",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs"
  },
  "keywords": [
    "oesp",
    "sdk",
    "did",
    "crypto",
    "bluetooth",
    "sync"
  ],
  "author": "OESP Team",
  "license": "MIT",
  "dependencies": {
    "@oesp/core": "workspace:*",
    "@oesp/crypto-sodium": "workspace:*",
    "@oesp/keystore-node": "workspace:*",
    "@oesp/storage-memory": "workspace:*",
    "@oesp/sync-http": "workspace:*",
    "@oesp/transport-ble-gatt": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "libsodium-wrappers-sumo": "^0.7.14 || ^0.8.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  }
}